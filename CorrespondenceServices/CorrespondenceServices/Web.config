    </logger>
    <logger name="CorrespondenceTrace">
      <priority value="ALL" />
      <level value="ALL" />
      <appender-ref ref="RestServiceTraceFileAppender" />
    </logger>
    <appender name="RestServiceTraceFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="E:\Logs\CorrespondenceTrace.log" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="2" />
      <maximumFileSize value="1MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger %ndc - %message%newline" />
      </layout>
    </appender>
    <appender name="RestServiceFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="E:\Logs\Correspondence.log" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
