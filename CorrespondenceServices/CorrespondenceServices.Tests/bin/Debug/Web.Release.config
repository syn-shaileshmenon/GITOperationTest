<?xml version="1.0"?>

<!-- For more information on using Web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=301874 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="UWManualWebApiUrl" value="https://uwdeliverysvc.emarkel.com:443/api/" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="DecisionServices" value="https://uwdecisionsvc.emarkel.com:443/api/" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="error:FilePath" value="E:\Logs\" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
    <add key="EchoServicesUrl" value="http://localhost:5000/api/" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="EchoService:Enabled" value="false" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />

    <httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" xdt:Transform="Insert" />
    </httpModules>
  </system.web>
  <storageProvider preferredStorageMode="FileSystem" xdt:Transform="Replace">
    <fileSystem uncPath="\\ecapp721\Documents\" groupingPattern="{SubmissionId}\{DocumentCreationDate}" compressionLevel="5">
    </fileSystem>
    <agentSignature uncPath="\\ecapp721\Documents\Signatures\" groupingPattern="{AgencyId}">
    </agentSignature>
    <corporateSignature uncPath="\\ecapp721\Documents\Signatures\">
    </corporateSignature>
    <IssuanceDoc uncPath="\\ecapp721\Documents\Issuance\" groupingPattern="{PolicyId}">
    </IssuanceDoc>
  </storageProvider>
  <log4net xdt:Transform="Replace">
    <logger name="Correspondence">
      <priority value="WARN" />
      <level value="WARN" />
      <appender-ref ref="RestServiceFileAppender" />
    </logger>
    <logger name="CorrespondenceTrace">
      <priority value="ALL" />
      <level value="ALL" />
      <appender-ref ref="RestServiceTraceFileAppender" />
    </logger>
    <appender name="RestServiceTraceFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="E:\Logs\CorrespondenceTrace.log" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="5" />
      <maximumFileSize value="5MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger %ndc - %message%newline" />
      </layout>
    </appender>
    <appender name="RestServiceFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="E:\Logs\Correspondence.log" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="5" />
      <maximumFileSize value="5MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger %ndc - %message%newline" />
      </layout>
    </appender>
  </log4net>
</configuration>
