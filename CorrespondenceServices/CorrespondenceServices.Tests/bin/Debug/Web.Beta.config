<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="DecisionServices" value="http://10.65.134.210:5080/api/" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="UWManualWebApiUrl" value="http://10.65.134.210:27080/api/" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="tempPath" value="\temp" />
    <add key="saveToFile" value="false" />
    <add key="error:FilePath" value="E:\Logs\" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>
  <log4net xdt:Transform="Replace">
    <logger name="Correspondence">
      <priority value="ALL" />
      <level value="WARN" />
      <appender-ref ref="RestServiceFileAppender" />
    </logger>
    <logger name="CorrespondenceTrace">
      <priority value="ALL" />
      <level value="WARN" />
      <appender-ref ref="RestServiceTraceFileAppender" />
    </logger>
    <appender name="RestServiceTraceFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="E:\Logs\CorrespondenceTraceBeta.log" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="2" />
      <maximumFileSize value="5MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger %ndc - %message%newline" />
      </layout>
    </appender>
    <appender name="RestServiceFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="E:\Logs\CorrespondenceBeta.log" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="2" />
      <maximumFileSize value="5MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger %ndc - %message%newline" />
      </layout>
    </appender>
  </log4net>
  <storageProvider preferredStorageMode="FileSystem" xdt:Transform="Replace">
    <fileSystem uncPath="E:\Documents\Issuance" groupingPattern="{SubmissionId}\{DocumentCreationDate}" compressionLevel="5">
    </fileSystem>
    <agentSignature uncPath="E:\Documents\Signatures" groupingPattern="{AgencyId}">
    </agentSignature>
    <corporateSignature uncPath="E:\Documents\Signatures\">
    </corporateSignature>
    <IssuanceDoc uncPath="E:\Documents\Issuance" groupingPattern="{PolicyId}">
    </IssuanceDoc>
  </storageProvider>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
</configuration>